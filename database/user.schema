CREATE TABLE users (
    id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
    email VARCHAR(100),
    password VARCHAR(128)
);

CREATE TABLE already_seen (
    id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
    user_id INT NOT NULL,
    dentist_id INT NOT NULL,
    FOREIGN KEY fk_user(user_id)
    FOREIGN KEY fk_dentist(dentist_id)
);
